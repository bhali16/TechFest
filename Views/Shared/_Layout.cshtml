<!DOCTYPE html>
<style>
</style>
<html lang="en">

<head>

    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="">
    <meta name="author" content="">
    <title>@ViewBag.Title</title>

    <!-- Bootstrap core CSS -->
    <link href="~/WebsiteTemplate/Content/vendor/bootstrap/css/bootstrap.min.css" rel="stylesheet">

    <!-- Custom fonts for this template -->
    <link href="~/WebsiteTemplate/Content/vendor/font-awesome/css/font-awesome.min.css" rel="stylesheet" type="text/css">
    <link href='https://fonts.googleapis.com/css?family=Open+Sans:300italic,400italic,600italic,700italic,800italic,400,300,600,700,800' rel='stylesheet' type='text/css'>
    <link href='https://fonts.googleapis.com/css?family=Merriweather:400,300,300italic,400italic,700,700italic,900,900italic' rel='stylesheet' type='text/css'>

    <!-- Plugin CSS -->
    <link href="@Url.Content("~/WebsiteTemplate/Content/vendor/magnific-popup/magnific-popup.css")" rel="stylesheet">

    <!-- Custom styles for this template -->
    <link href="@Url.Content("~/WebsiteTemplate/Content/css/creative.min.css")" rel="stylesheet">
    @Styles.Render("~/~/WebsiteTemplate/Content/css")
    @Scripts.Render("~/bundles/modernizr")

    <script src="~/Scripts/jquery-3.4.1.min.js"></script>
    <script src="~/Scripts/popper.min.js"></script>
</head>

<body id="page-top" style="margin:0;height:100%">


    <!-- Navigation -->
    <nav class="navbar navbar-expand-lg navbar-light fixed-top" id="mainNav">
        <div class="container" style="max-width: 1140px;position: relative;min-height: 100%;">
            <a class="navbar-brand js-scroll-trigger" href="#page-top">TechFest</a>
            <button class="navbar-toggler navbar-toggler-right" type="button" data-toggle="collapse" data-target="#navbarResponsive" aria-controls="navbarResponsive" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>
            <div class="collapse navbar-collapse" id="navbarResponsive">
                <ul class="navbar-nav ml-auto">
                    <li class="nav-item">
                        @Html.ActionLink("Home", "Index", "Home", null, new { @class = "nav-link js-scroll-trigger" })
                    </li>
                    <li class="nav-item">
                        <a class="nav-link js-scroll-trigger" href="#about">About</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link js-scroll-trigger" href="#services">Services</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link js-scroll-trigger" href="#contact">Contact</a>
                    </li>

                    <li class="nav-item">
                        @Html.ActionLink("Organizer", "Index", "Organizer", null, new { @class = "nav-link js-scroll-trigger" })
                    </li>
                    <li class="nav-item">
                        @Html.ActionLink("Heads", "Index", "EventHeads", null, new { @class = "nav-link js-scroll-trigger" })
                    </li>
                    <li class="nav-item">
                        @Html.ActionLink("Participant", "Index", "Participant", null, new { @class = "nav-link js-scroll-trigger" })
                    </li>
                </ul>
            </div>
        </div>
    </nav>
    @RenderBody()
    <!-- Bootstrap core JavaScript -->
    <script src="~/WebsiteTemplate/Content/vendor/jquery/jquery.min.js"></script>
    <script src="@Url.Content("~/WebsiteTemplate/Content/vendor/bootstrap/js/bootstrap.bundle.min.js")"></script>

    <!-- Plugin JavaScript -->
    <script src="@Url.Content("~/WebsiteTemplate/Content/vendor/jquery-easing/jquery.easing.min.js")"></script>
    <script src="@Url.Content("~/WebsiteTemplate/Content/vendor/scrollreveal/scrollreveal.min.js")"></script>
    <script src="@Url.Content("~/WebsiteTemplate/Content/vendor/magnific-popup/jquery.magnific-popup.min.js")"></script>

    <!-- Custom scripts for this template -->
    <script src="@Url.Content("~/WebsiteTemplate/Content/js/creative.min.js")"></script>
    <script src="@Url.Content("~/bundles/jqueryval")"></script>
    <script src="@Url.Content("~/Scripts/jquery-3.4.1.min.js")"></script>
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)

</body>

</html>
